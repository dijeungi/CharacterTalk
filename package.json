{
  "name": "againhello",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:init": "dotenv -e .env -- bash -c 'psql -U $PGUSER -d $PGDATABASE -f db/schema.sql && psql -U $PGUSER -d $PGDATABASE -f db/seed.sql'",
    "setup": "npm install && npm run db:init && npm run dev",
    "//": [
      "실행 명령어 정리",
      "npm run setup     # 전체 설치 + DB 초기화 + dev 실행",
      "npm run db:init    # DB 테이블/시드 수동 초기화",
      "npm run dev        # 개발 서버 실행"
    ]
  },
  "dependencies": {
    "@tanstack/react-query": "^5.75.5",
    "axios": "^1.9.0",
    "firebase": "^11.7.3",
    "install": "^0.13.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "next": "15.3.2",
    "node-fetch": "^3.3.2",
    "npm": "^11.3.0",
    "pg": "^8.15.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-spinners": "^0.17.0",
    "sweetalert2": "^11.21.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "typescript": "^5"
  }
}
